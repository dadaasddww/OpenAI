<Window x:Class="OpenAI_API.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenAI_API"
        mc:Ignorable="d"
        Title="Main" Height="250" Width="400" Icon="/Icon1.ico" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Margin ="10">
            <Button x:Name="Button_chat" Content="Chat" Height="50" Width="80" Margin="5" Click="Button_chat_Click"></Button>
            <Button x:Name="Button_image" Content="Image" Height="50" Width="80" Margin="5" Click="Button_image_Click" ></Button>
        </StackPanel>
    </Grid>
</Window>
